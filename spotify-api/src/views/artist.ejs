<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Title</title>
    </head>
    <body>
        <ul>
            <% artists.forEach(function(artist) { %>
            <div>
                <a href="http://localhost:9977/artists/<%=artist.id%>"
                    ><%= artist.name%></a
                >
                <img src="<%= artist.images[0] ? artist.images[0].url : "" %>"
                alt="" />
            </div>
            <% }); %>
        </ul>
    </body>
</html>
